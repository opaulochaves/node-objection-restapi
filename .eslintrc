{
  "extends": ["standard", "prettier", "prettier/standard"],
  "plugins": ["import", "prettier", "standard"],
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "space-before-function-paren": 0,
    "new-cap": 0,
    "prettier/prettier": 2,
    "no-use-before-define": 0
  }
}
